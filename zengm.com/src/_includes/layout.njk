<!doctype html>
<html lang="en">
  <head>
    {% include "head-common.njk" %}
    <title>ZenGM > {{ title }}</title>
    <style>
#face {
  height: 48px;
  width: 36px;
	cursor: pointer;
	user-select: none;
	-webkit-user-select: none;
}

.top-nav {
  background-color: #3b3b3b;
}

.breadcrumb {
  background-color: inherit;
}
    </style>
  </head>
  <body>

<div class="mb-3 top-nav">
  <div class="container d-flex">
    <div id="face"></div>
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb mb-0">
        <li class="breadcrumb-item"><a href="/">ZenGM</a></li>
        <li class="breadcrumb-item active" aria-current="page">{{ title }}</li>
      </ol>
    </nav>
  </div>
</div>
      
<main role="main" class="container">
  {{ content | safe }}
</main>

</body>
{% include "foot-common.njk" %}
{% set js %}
    {% include "layout.js" %}
{% endset %}
<script>
    {{ js | jsmin | safe }}
</script>
</html>
